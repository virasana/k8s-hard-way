{
  "CN": "kubernetes",
  "hosts": [
    "${dns_etcd_srv_name}.${environment}",
    "controller0.${environment}",
    "controller1.${environment}",
    "controller2.${environment}",
    "worker0.${environment}",
    "worker1.${environment}",
    "worker2.${environment}",
    "${kubernetes_public_ip_address}",
    "${network_ip_controller}0",
    "${network_ip_controller}1",
    "${network_ip_controller}2",
    "${network_ip_worker}0",
    "${network_ip_worker}1",
    "${network_ip_worker}2",
    "127.0.0.1"
  ],
  "key": {
    "algo": "rsa",
    "size": 2048
  },
  "names": [
    {
      "C": "${cert_country}",
      "L": "${cert_locality}",
      "O": "${cert_organisation_name}",
      "OU": "${cert_organisational_unit}",
      "ST": "${cert_state}"
    }
  ]
}